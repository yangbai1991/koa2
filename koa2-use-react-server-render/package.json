{
  "name": "koa2-use-react-server-render",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon app.js --exec babel-node --presets env,react",
    "build": "rm -rf dist && babel --ignore node_modules ./ -d dist --presets env,react --plugins transform-runtime",
    "server": "node dist/app.js"
  },
  "license": "MIT",
  "dependencies": {
    "koa": "^2.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.12.1",
    "opn": "^5.1.0"
  }
}
